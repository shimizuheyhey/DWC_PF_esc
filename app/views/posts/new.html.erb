<h1>新規投稿</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
<h5>CutTime</h5>
<%= f.fields_for :cut_times, local:true do |ff| %>
  <h4>分</h4>
  <%= ff.number_field :minute %>
  <h4>秒</h4>
  <%= ff.number_field :second %>
  <% end %>
  <%= f.submit '投稿' %>
  <% end %>