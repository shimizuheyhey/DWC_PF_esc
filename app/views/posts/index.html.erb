<% @tags.each do |tag| %>
  <span style="background-color: #e9e9e9; border-radius: 5px;">
    <%#= link_to "#{tag.name}(#{tag.taggings_count})", tags_path(tag.name) %>
    </span>
<% end %>
<% @post.each do |post| %>
<div>
  <%#= attachment_image_tag post_image, :image %>
  <%#><p>投稿ユーザー画像：<%= image_tag('sample-author1.jpg') %></p>

  <%= link_to post.title, post_path(post) %>

  <p>ユーザーネーム：<%= post.user.name %></p>

</div>
<% end %>

